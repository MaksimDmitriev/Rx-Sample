<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.maksim.rxsample">

    <application
        android:name="ru.maksim.rxsample.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ru.maksim.rxsample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="ru.maksim.rxsample.TraditionalApproachActivity"
            android:label="TraditionalApproachActivity" />
        <activity
            android:name="ru.maksim.rxsample.RxApproachActivity"
            android:label="RxApproachActivity" />
        <activity android:name=".SimpleActivity" />
    </application>

</manifest>